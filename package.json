{
  "name": "sayagata-leds",
  "version": "1.0.0",
  "description": "Backend for the control of the LEDs in Sayagat",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "run-s -l build:*",
    "build:type-check": "tsc --noEmit",
    "build:clean": "rimraf dist",
    "build:run-babel-node": "babel src -d dist --extensions \".ts\""
  },
  "keywords": [
    "genart",
    "led",
    "sayagata"
  ],
  "author": "Aitor Carlos Urrutia Aranburu",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@types/node": "^18.15.11",
    "fastify": "^4.15.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
